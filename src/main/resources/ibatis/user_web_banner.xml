<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE sqlMap       
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"       
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="user_web_banner">
	
	<select id="getOnlineList" resultClass="string" parameterClass="java.util.Map">
		select picUrl from user_web_banner where user_id=#userId# and status=#status#;
	</select>
	
</sqlMap>  